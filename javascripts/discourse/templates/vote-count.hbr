{{!-- has-modern-replacement --}}
{{#if view.showVoteCount}}
  <div class='vote-count-before-title'>
    <button class="topic-list-vote-button btn-flat {{view.userVotedClass}} {{view.votingDisabled}}" title="{{view.votedStatus}}">{{d-icon 'caret-up'}}</button>
    <span class="vote-count-value">{{view.voteCount}}</span>
  </div>
{{/if}}